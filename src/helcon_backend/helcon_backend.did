type Appointment = record {
  id : nat64;
  patient_id : nat64;
  multimedia_content : opt MultiMediaContent;
  date_time : nat64;
  doctor_id : nat64;
  reason : text;
};
type Calendly = record { id : nat64; principle_id : text; calendly : text };
type Data = record {
  id : nat64;
  data : vec nat8;
  patient_username : text;
  doctor_username : text;
};
type DocIdentity = record { id : nat64; "principal" : text };
type Doctor = record {
  id : nat64;
  dob : text;
  sex : text;
  fname : text;
  country : text;
  city : text;
  licence_no : nat64;
  specialism : text;
  lname : text;
  id_no : nat64;
  principal_str : text;
};
type Error = variant {
  InvalidInput : record { msg : text };
  AppointmentConflict : record { msg : text };
  NotFound : record { msg : text };
  Unauthorized : record { msg : text };
  AlreadyExists : record { msg : text };
};
type Identity = record { id : nat64; "principal" : text };
type MedicalRecord = record {
  id : nat64;
  patient_id : nat64;
  lab_results : text;
  treatment_history : text;
};
type Message = record {
  id : nat64;
  receiver_id : nat64;
  content : text;
  multimedia_content : opt MultiMediaContent;
  sender_id : nat64;
};
type MultiMediaContent = record {
  image_url : opt text;
  audio_url : opt text;
  video_url : opt text;
};
type Patient = record { id : nat64; username : text; identity_id : nat64 };
type Report = record {
  id : nat64;
  patient_id : nat64;
  username : text;
  multimedia_content : opt MultiMediaContent;
  prescription : text;
  diagnostic : text;
  recommendations : text;
  symptoms : text;
};
type Result = variant { Ok : Calendly; Err : Error };
type Result_1 = variant { Ok : Data; Err : Error };
type Result_10 = variant { Ok : Patient; Err : Error };
type Result_2 = variant { Ok : DocIdentity; Err : Error };
type Result_3 = variant { Ok : Doctor; Err : Error };
type Result_4 = variant { Ok : Identity; Err : Error };
type Result_5 = variant { Ok : Report; Err : Error };
type Result_6 = variant { Ok : MedicalRecord; Err : Error };
type Result_7 = variant { Ok; Err : Error };
type Result_8 = variant { Ok : Appointment; Err : Error };
type Result_9 = variant { Ok : Message; Err : Error };
service : {
  add_calendly : (text, text) -> (Result);
  add_data : (text, text, vec nat8) -> (Result_1);
  add_docidentity : (text) -> (Result_2);
  add_doctor : (
      text,
      text,
      text,
      text,
      text,
      nat64,
      nat64,
      text,
      text,
      text,
    ) -> (Result_3);
  add_identity : (text) -> (Result_4);
  add_report : (nat64, text, text, text, text, text, opt MultiMediaContent) -> (
      Result_5,
    );
  create_medical_record : (nat64, nat64, text, text) -> (Result_6);
  delete_calendly : (nat64) -> (Result_7);
  delete_docidentity : (nat64) -> (Result_7);
  delete_doctor : (nat64) -> (Result_7);
  delete_identity : (nat64) -> (Result_7);
  delete_medical_record : (nat64) -> (Result_7);
  delete_message : (nat64) -> (Result_7);
  delete_report : (nat64) -> (Result_7);
  does_docidentity_exist : (text) -> (bool) query;
  does_identity_exist : (text) -> (bool) query;
  get_appointment : (nat64) -> (Result_8) query;
  get_calendly : (nat64) -> (Result) query;
  get_data : (nat64) -> (Result_1) query;
  get_docidentity : (nat64) -> (Result_4) query;
  get_doctor : (nat64) -> (Result_3) query;
  get_identity : (nat64) -> (Result_4) query;
  get_medical_record : (nat64) -> (Result_6) query;
  get_message : (nat64) -> (Result_9) query;
  get_patient : (nat64) -> (Result_10) query;
  get_report : (nat64) -> (Result_5) query;
  list_appointments : () -> (vec Appointment) query;
  list_docidentities : () -> (vec Identity) query;
  list_doctors : () -> (vec Doctor) query;
  list_identities : () -> (vec Identity) query;
  list_medical_records : () -> (vec MedicalRecord) query;
  list_messages : () -> (vec Message) query;
  list_reports : () -> (vec Report) query;
  register_patient : (text, nat64) -> (Result_10);
  schedule_appointment : (nat64, nat64, nat64, text, opt MultiMediaContent) -> (
      Result_8,
    );
  send_message : (nat64, nat64, text, opt MultiMediaContent) -> (Result_9);
  send_reminder_to_patient : (nat64, text, opt MultiMediaContent) -> (Result_9);
  update_doctor : (
      text,
      text,
      text,
      text,
      text,
      nat64,
      nat64,
      text,
      text,
      text,
    ) -> (Result_3);
  update_medical_record : (nat64, nat64, text, text) -> (Result_6);
  update_message : (nat64, nat64, nat64, text, opt MultiMediaContent) -> (
      Result_9,
    );
  update_report : (
      nat64,
      nat64,
      text,
      text,
      text,
      text,
      text,
      opt MultiMediaContent,
    ) -> (Result_5);
}
